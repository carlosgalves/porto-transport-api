# Production Environment Configuration
# Copy this file to .env.production
# Note: On Railway, environment variables are set automatically and will override these values

# Database Configuration (Railway provides DATABASE_URL automatically)
# DATABASE_URL=postgresql://user:password@host:port/dbname

# API Configuration
API_HOST=0.0.0.0
API_PORT=8000

# GTFS Data Directory (optional, defaults to data/raw/stcp)
# GTFS_DATA_DIR=data/raw/stcp

# Environment
ENVIRONMENT=production
